@article{yuObtainingPlotwiseMean2008,
  title = {Obtaining Plotwise Mean Height and Volume Growth in Boreal Forests Using Multi-temporal Laser Surveys and Various Change Detection Techniques},
  author = {Yu, X. and Hyypp{\"a}, J. and Kaartinen, H. and Maltamo, M. and Hyypp{\"a}, H.},
